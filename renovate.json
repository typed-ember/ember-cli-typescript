{
  "extends": [
    "config:js-lib",
    ":pinOnlyDevDependencies",
    ":automergePatch",
    ":automergeMinor",
    ":semanticCommits"
  ],
  "packageRules": [
    {
      "packageNames": [
        "typescript",
        "tslint",
        "typedoc"
      ],
      "groupName": "typescript packages",
      "groupSlug": "typescript",
      "minor": {
        "automerge": false
      }
    },
    {
      "packageNames": [
        "^@types"
      ],
      "groupName": "ambient types",
      "groupSlug": "ambient-types",
      "minor": {
        "automerge": false
      }
    },
    {
      "packageNames": [
        "broccoli-asset-rev",
        "ember-cli",
        "ember-cli-dependency-checker",
        "ember-cli-eslint",
        "ember-cli-eslint",
        "ember-qunit",
        "ember-cli-sri",
        "qunit-dom",
        "ember-cli-fastboot",
        "ember-cli-htmlbars-inline-precompile",
        "ember-cli-inject-live-reload",
        "ember-cli-qunit",
        "ember-cli-shims",
        "ember-cli-release",
        "ember-cli-uglify",
        "ember-disable-prototype-extensions",
        "ember-export-application-global",
        "ember-load-initializers",
        "ember-resolver",
        "ember-source",
        "eslint-plugin-ember",
        "eslint-plugin-node",
        "loader.js",
        "ember-cli-htmlbars",
        "ember-cli-babel"
      ],
      "groupName": "stable ember infrastructure",
      "groupSlug": "stable-ember-infra",
      "major": {
        "automerge": true
      }
    }
  ]
}
